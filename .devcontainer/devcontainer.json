{
    "name": "devin.cursorrules",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "dev",
    "workspaceFolder": "/app",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.vscode-pylance",
                "ms-azuretools.vscode-docker"
            ]
        }
    },
    "forwardPorts": [8000],
    "remoteUser": "appuser",
    "postCreateCommand": "pip install -r requirements.txt",
    "overrideCommand": false
}